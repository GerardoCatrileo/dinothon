<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
	<mapper namespace="com.hackaton.dinothon.mapper.DespachoMapper">
		

	<resultMap id="DespachoMap" type="com.hackaton.dinothon.entity.Despacho" >
	  <id 		property="id_desp" 		column="id_despacho" 	javaType="java.lang.Long"/>
	  <result	property="f12" 		column="n_f12" 	javaType="java.lang.String" 	jdbcType="VARCHAR"/>
	</resultMap>
	
	 <select id = "getDespachoByF12" resultMap="DespachoMap" flushCache="true">
        select id_despacho, n_f12 from hck_despacho hd
		where n_f12 = #{f12}
    </select>

</mapper>